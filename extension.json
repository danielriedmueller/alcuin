{
	"name": "Alcuin",
	"author": [
		"Daniel RiedmÃ¼ller"
	],
	"url": "https://www.soital.de",
	"descriptionmsg": "alcuin-desc",
	"license-name": "GPL-3.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"AutoloadClasses": {
		"Alcuin": "includes/Alcuin.php",
		"AlcuinHooks": "includes/Hooks.php",
		"SpecialHelloWorld": "includes/SpecialHelloWorld.php"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\Alcuin\\": "includes/"
	},
	"config": {
		"AlcuinVandalizeEachPage": {
			"description": "Write 'Alcuin was here' on each page",
			"value": true
		}
	},
	"callback": "AlcuinHooks::onExtensionLoad",
	"HookHandlers": {
		"AlcuinHooks": {
			"class": "MediaWiki\\Extension\\Alcuin\\Hooks"
		}
	},
	"Hooks": {
		"BeforePageDisplay": "AlcuinHooks"
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "Alcuin/modules"
	},
	"ResourceModules": {
		"ext.alcuin": {
			"scripts": [
				"resources/script.js"
			],
			"styles": [
				"resources/style.css"
			]
		}
	},
	"SpecialPages": {
		"HelloWorld": "SpecialHelloWorld"
	},
	"manifest_version": 2
}