{
	"name": "Alcuin",
	"author": [
		"Daniel RiedmÃ¼ller"
	],
	"url": "https://www.soital.de",
	"descriptionmsg": "alcuin-desc",
	"license-name": "GPL-3.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extensions\\Alcuin\\": "includes/"
	},
	"config": {
		"AlcuinVandalizeEachPage": {
			"description": "Write 'Alcuin was here' on each page",
			"value": true
		}
	},
	"Hooks": {
		"BeforePageDisplay": "main"
	},
	"HookHandlers": {
		"main": {
			"class": "MediaWiki\\Extension\\Alcuin\\HookHandler"
		}
	},
	"ResourceModules": {
		"ext.alcuin": {
			"scripts": "script.js",
			"styles": "style.less"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "Alcuin/resources"
	},
	"manifest_version": 2
}